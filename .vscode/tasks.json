{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "server",
            "type": "shell",
            "command": "cd shop-back && npm start",
            "problemMatcher": [
                "$eslint-stylish"
            ]
        },
        {
            "label": "static",
            "type": "shell",
            "command": "cd shop-front && npm run build",
            "problemMatcher": [
                "$eslint-stylish"
            ]
        },
        {
            "label": "fill db",
            "type": "shell",
            "command": "cd shop-back && node fill_db.js",
            "problemMatcher": [
                "$eslint-stylish"
            ]
        },
        {
            "label": "front dev",
            "type": "shell",
            "command": "cd shop-front && npm run dev",
            "problemMatcher": [
                "$eslint-stylish"
            ]
        }
    ]
}